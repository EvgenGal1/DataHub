{
  "version": 2,
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts"
    },
    {
      "src": "/img/(.*)",
      "dest": "/public/img/$1"
    },
    {
      "src": "/swagger/(.*)",
      "dest": "/public/swagger/$1"
    },
    {
      "src": "/pages/(.*)",
      "dest": "/public/views/$1"
    },
    {
      "src": "/pages/(.*)",
      "dest": "/public/views/pages/$1"
    }
  ]
}
