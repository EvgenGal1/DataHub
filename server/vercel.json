{
  "version": 2,
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/swagger/(.*)",
      "dest": "/swagger/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/main.js"
    }
  ]
}
