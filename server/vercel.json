{
  "version": 2,
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    },
    {
      "src": "/api/(.*)",
      "dest": "src/main.ts"
    },
    {
      "src": "/swagger/(.*)",
      "dest": "src/public/swagger/$1"
    },
    {
      "src": "/swagger/(.*)",
      "dest": "src/public/swagger/swagger.json"
    },
    {
      "src": "/api/(.*)",
      "dest": "src/main.ts"
    },
    {
      "src": "/swagger/(.*)",
      "dest": "node_modules/swagger-ui-dist/$1"
    }
  ]
}
